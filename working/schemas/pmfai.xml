			<title>Electronic Product Environmental Assessment Tool Rating</title>
			<type>string</type>
			<description></description>
			<constraints>
				<pattern level="error">Bronze|Silver|Gold|N/A</pattern>
			</constraints>
			<alias>EPEAT_Rating</alias>
			<alias>EPEAT Rating (Select from Drop Down)</alias>
		</field>	
		<field>
			<name>AGENCY</name>
			<title>Agency</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required>true</required>
			</constraints>
			<alias>Agency</alias>
			<alias>Agency&#9094;(Select from Drop Down List)</alias>
		</field>				
		<field>
			<name>SUB_AGENCY</name>
			<title>Sub Agency or Bureau</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required>true</required>
			</constraints>
			<alias>sub_agency</alias>
			<alias>Sub-Agency</alias>
		</field>	
		<field>
			<name>POC_Email</name>
