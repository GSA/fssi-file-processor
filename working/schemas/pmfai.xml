			<type>number</type>
			<description></description>
			<alias>Maint_Schedule_BW_Rate</alias>
			<alias>Schedule Click Rates B/W Rate</alias>						
		</field>		
		<field>
			<name>MAINTENANCE_SCHEDULE_COLOR_PRICE</name>
			<title>Maintenance Schedule Click Rate for Color</title>
			<type>number</type>
			<description></description>
			<alias>Maint_Schedule_Color_Rate</alias>
			<alias>Schedule Click Rates Color Rate</alias>						
		</field>																																								
		<field>
			<name>MAINTENANCE_BPA_BW_PRICE</name>
			<title>Maintenance BPA Click Rate for B/W</title>
			<type>number</type>
			<description></description>
			<alias>Maint_BPA_BW_Rate</alias>
			<alias>BPA Click Rates B/W Rate</alias>						
		</field>		
		<field>
			<name>MAINTENANCE_BPA_COLOR_PRICE</name>
			<title>Maintenance BPA Click Rate for Color</title>
			<type>number</type>
			<description></description>
			<alias>Maint_BPA_Color_Rate</alias>
			<alias>BPA Click Rates Color Rate</alias>			
		</field>
		<field>
			<name>MAINTENANCE_TASK_ORDER_BW_PRICE</name>
