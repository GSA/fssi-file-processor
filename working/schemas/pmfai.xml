			<title>Device (Lease/Purchase) Schedule Price</title>
			<type>number</type>
			<description></description>
			<alias>Dev_Schedule_Price</alias>
			<alias>Schedule Price&#9094;(Lease/Purchase)</alias><!-- Need to figure out "Enter code" -->
		</field>
		<field>
			<name>DEVICE_BPA_PRICE</name>
			<title>Device (Lease/Purchase) BPA Price</title>
			<type>number</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Dev_BPA_Price</alias>
			<alias>BPA Price&#9094;(Lease/Purchase)</alias><!-- Need to figure out "Enter code" -->
		</field>	
		<field>
			<name>DEVICE_TASK_ORDER_PRICE</name>
			<title>Device (Lease/Purchase) Task Order Price</title>
			<type>number</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Dev_Task_Price</alias>
			<alias>Task Order Price&#9094;(Lease/Purchase)</alias><!-- Need to figure out "Enter code" -->
		</field>	
		<field>
			<name>MAINTENANCE_SCHEDULE_PRICE</name>
			<title>Maintenance Schedule Price (Base Fee)</title>
			<type>number</type>
			<description></description>
			<alias>Maint_Schedule_Price</alias>
			<alias>Schedule Price&#9094;(Base Fee)</alias><!-- Need to figure out "Enter code" -->
		</field>	
		<field>
			<name>MAINTENANCE_BPA_PRICE</name>
			<title>Maintenance BPA Price (Base Fee)</title>
			<type>number</type>
			<description></description>
			<alias>Maint_BPA_Price</alias>
			<alias>BPA Price&#9094;(Base Fee)</alias><!-- Need to figure out "Enter code" -->
		</field>
		<field>
			<name>MAINTENANCE_TASK_ORDER_PRICE</name>
			<title>Maintenance BPA Price (Base Fee)</title>
			<type>number</type>
			<description></description>
			<alias>Maint_Task_Price</alias>
			<alias>Task Order Price&#9094;(Base Fee)</alias><!-- Need to figure out "Enter code" -->
		</field>	
