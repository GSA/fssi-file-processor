<?xml version="1.0" encoding="UTF-8"?>
<schema>
	<name>PM_FAI</name>
	<provider>PM</provider>
	<version>1.0</version>
	<title>Print Management Functional Area I</title>
	<fields>
		<field>
			<name>BPA_NUMBER</name>
			<title>BPA Number</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>BPA Number                                    (Select from Drop Down List)</alias>
			<alias>BPA_No</alias>
		</field>
		<field>
			<name>TASK_ORDER_NUMBER</name>
			<title>Task Order Number</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Task_Order_No</alias>
			<alias>Task Order Number                             *Do not include dashes or spaces</alias>
		</field>	
		<field>
			<name>DEVICE_MODEL</name>
			<title>Device Model</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Device_Model</alias>
			<alias>Device Model                                 *Do not include dashes or spaces</alias>
		</field>
		<field>
			<name>VOLUME_BAND</name>
			<title>Volume Band</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Volume Band</alias>
			<alias>Volume_Band</alias>
		</field>	
		<field>
			<name>QUANTITY_OF_DEVICES</name>
			<title>Quantity of Devices</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Qty of Devices</alias>
			<alias>Device_Quantity</alias>
		</field>	
		<field>
			<name>FUNCTIONAL_AREA</name>
			<title>Functional Area</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
				<pattern level="fatal">FA I|FA II|FA III|FA IV</pattern>
			</constraints>
			<alias>FA</alias>
			<alias>Functional &#9094; Area</alias> <!-- Need to figure out "Enter code" -->
		</field>		
		<field>
			<name>BRIEF_DESCRIPTION</name>
			<title>Brief Description</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Brief_Desc</alias>
			<alias>Brief Description &#9094; (Select from Drop Down List)</alias><!-- Need to figure out "Enter code" -->
		</field>	
		<field>
			<name>PURCHASE_TYPE</name>
			<title>Purchase Type</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Purchase_Type</alias>
			<alias>Type of Purchase &#9094; (Select from Drop Down List)</alias><!-- Need to figure out "Enter code" -->
		</field>			
		<field>
			<name>AGREEMENT_LENGTH</name>
			<title>AGREEMENT Length</title>
			<type>string</type>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Agreement_Length</alias>
			<alias>Length of Lease/Maintenance&#9094;(if applicable)</alias><!-- Need to figure out "Enter code" -->
		</field>	
		<field>
			<name>MONTH_NAME</name>
			<title>Month Name</title>
			<type>string</type>
			<description></description>
			<alias>Month</alias>
			<alias>To:                    Month                                                (Select from Drop Down List)</alias>
		</field>	
		<field>
			<name>CALENDAR_YEAR</name>
			<title>Calendar Year</title>
			<type>string</type>
			<description></description>
			<alias>Year</alias>
			<alias>To:                         Year                                                (Select from Drop Down List)</alias>
		</field>
		<field>
			<name>EFFECTIVE_DATE</name>
			<title>Effective Date</title>
			<type>Date</type>
			<format>MM/dd/yyyy</format>
			<description></description>
			<constraints>
				<required level="error">true</required>
			</constraints>
			<alias>Effective Date of purchase/lease (date installed at customer site)</alias>
		</field>
		<field>
