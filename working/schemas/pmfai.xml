			<title>Green Product Indicator</title>
			<type>string</type>
			<description></description>
			<constraints>
				<pattern level="error">Yes|No</pattern>
			</constraints>
			<alias>Green_Product</alias>
			<alias>Is this product registered on Advantage as a Green Product? (Select from Drop Down)</alias>
		</field>
		<field>
